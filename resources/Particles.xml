  <emitter id="BoxDead" shape="CIRCLE" dispersion="RADIAL">
    <textRect x="32" y="96" width="16" height="16"/>
    <ranges>
      <focusTOL min="0.1" max="0.1"/>
      <focusWidth min="0.3" max="0.3"/>
      <focusHeight min="0.3" max="0.3"/>
      <focusNP min="650" max="1000"/>
      <focusPPS min="1000" max="1000"/>
